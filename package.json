{
  "name": "httpany",
  "version": "0.1.1",
  "description": "Generate any http status and headers",
  "main": "./lib/app.js",
  "bin": {
    "httpany": "./bin/cli.js"
  },
  "files":["src", "bin"],
  "scripts": {
    "postinstall": "npm run build",
    "test": "nyc ava ",
    "build": "babel ./src -d ./lib",
    "lint": "eslint ./src ./test --fix"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "babel": {
    "plugins": [
      "transform-async-to-generator",
      "transform-es2015-modules-commonjs"
    ]
  },
  "author": "flowmemo",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.15.1",
    "coveralls": "^2.11.9",
    "nyc": "^7.0.0"
  },
  "dependencies": {
    "babel-cli": "^6.10.1",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.10.3",
    "chalk": "^1.1.3",
    "koa": "^2.0.0",
    "koa-httpany": "^0.1.4",
    "koa-static": "^3.0.0",
    "meow": "^3.7.0",
    "node-fetch": "^1.5.3"
  }
}
